</style>

<script>
  var secondSwiper = null; // Declare a variable to hold the Swiper instance

  function initializeSwiper() {
      if (window.innerWidth <= 768) {
          if (secondSwiper === null) {
              secondSwiper = new Swiper(".mySwiper-second", {
                  slidesPerView: 1.2,
                  centeredSlides: true,
                  spaceBetween: 10,
                  pagination: {
                      el: ".swiper-pagination",
                      clickable: true,
                  },
                  navigation: {
                      nextEl: ".swiper-button-next",
                      prevEl: ".swiper-button-prev",
                  },
              });
          }
      } else {
          // Destroy Swiper instance if the window width is greater than 768 pixels
          if (secondSwiper !== null) {
              secondSwiper.destroy();
              secondSwiper = null;
              document.querySelector('.swiper-pagination span').style = "display: hidden";
          }
      }
  }

  window.addEventListener('resize', function() {
      initializeSwiper(); // Call the function when the window is resized
  });

  // Call the function initially to check the initial window width
  initializeSwiper();


    window.addEventListener('resize', function() {
        initializeSwiper(); // Call the function when the window is resized
    });

    // Call the function initially to check the initial window width
    initializeSwiper();



         let custom_customer_care = document.querySelectorAll('.custom-customer-care__iconwrapper');

        custom_customer_care.forEach(function(element, index) {
          let delay = index * 100; // Calculate delay based on index
          element.setAttribute('data-aos-delay', delay); // Set the data-aos-delay attribute
        });
</script>

